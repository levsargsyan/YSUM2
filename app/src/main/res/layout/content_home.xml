<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="am.ysu.ysum.HomeActivity"
    tools:showIn="@layout/app_bar_home"
    android:background="@android:color/white">


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="105dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        app:srcCompat="@drawable/ysu"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="30dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="150dp"
        android:layout_height="77dp"
        android:layout_marginLeft="7dp"
        android:text="Schedule"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="157dp" />

    <View
        android:layout_width="2dip"
        android:layout_height="100dip"
        android:background="#000000"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        android:id="@+id/view" />

    <Button
        android:id="@+id/button4"
        android:layout_width="150dp"
        android:layout_height="77dp"
        android:layout_marginRight="7dp"
        android:text="Examined"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="157dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="96dp"
        android:layout_height="93dp"
        android:layout_marginRight="25dp"
        android:scaleType="centerCrop"
        app:layout_constraintRight_toRightOf="parent"
        app:srcCompat="@drawable/yerevan_state_university_16"
        android:layout_marginBottom="24dp"
        app:layout_constraintBottom_toTopOf="@+id/textView7" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="96dp"
        android:layout_height="93dp"
        android:layout_marginRight="23dp"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/yerevan_state_university_16"
        android:layout_marginBottom="24dp"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintRight_toLeftOf="@+id/imageView4" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="96dp"
        android:layout_height="93dp"
        android:layout_marginRight="263dp"
        android:scaleType="centerCrop"
        app:layout_constraintRight_toRightOf="parent"
        app:srcCompat="@drawable/yerevan_state_university_16"
        android:layout_marginBottom="24dp"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="1.0" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:text="31.07.17"
        android:layout_marginLeft="42dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginBottom="14dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView6" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="8dp"
        android:text="31.07.17"
        app:layout_constraintLeft_toRightOf="@+id/textView2"
        android:layout_marginLeft="8dp"
        app:layout_constraintHorizontal_bias="0.482"
        app:layout_constraintRight_toLeftOf="@+id/textView4"
        android:layout_marginBottom="15dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView7" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="30.07.17"
        android:layout_marginRight="42dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="14dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView4" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="90dp"
        android:layout_height="35dp"
        android:layout_marginBottom="16dp"
        android:text="dfdfvffgggggggggggggggggggggggggggggggggggggggggggggggggggggggggggtttttttttttttyyyyyyyyyyy"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="25dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="90dp"
        android:layout_height="35dp"
        android:layout_marginBottom="16dp"
        android:text="dfvfdvdfvfdvfdvfdvfd"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="90dp"
        android:layout_height="35dp"
        android:layout_marginBottom="16dp"
        android:layout_marginRight="25dp"
        android:text="fgbthfbbhgbfgbfgvc "
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

</android.support.constraint.ConstraintLayout>
